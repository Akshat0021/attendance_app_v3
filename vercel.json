{
  "builds": [
    {
      "src": "api/get_embedding.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "." }
    }
  ],
  "routes": [
    {
      "src": "/api/get_embedding",
      "dest": "api/get_embedding.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
